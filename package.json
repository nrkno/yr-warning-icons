{
  "name": "yr-warning-icons",
  "version": "1.0.0",
  "description": "Warning icons for yr.no",
  "repository": "https://github.com/nrkno/yr-warning-icons",
  "scripts": {
    "clean": "rimraf dist/pdf/*.pdf dist/png/*.png dist/svg/*.svg temp/*.html",
    "preconvert-icons": "npm run clean && docker build -t yr-warning-icons .",
    "convert-icons": "docker run  -v $PWD/design/:/design -v $PWD/dist:/dist --rm --cap-add=SYS_ADMIN  yr-warning-icons"
  },
  "author": "Simen Saegrov",
  "license": "MIT",
  "devDependencies": {
    "@types/puppeteer": "1.11.1",
    "prettier": "1.15.3",
    "puppeteer": "1.11.0",
    "svgo": "1.1.1"
  },
  "dependencies": {
    "rimraf": "2.6.3"
  }
}
