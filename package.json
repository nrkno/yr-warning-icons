{
  "name": "yr-warning-icons",
  "version": "1.0.0",
  "description": "Warning icons for yr.no",
  "main": "index.js",
  "scripts": {
    "clean": "rimraf dist/pdf/*.pdf dist/png/*.png dist/svg/*.svg temp/",
    "preconvert-icons": "docker build -t yr-warning-icons .",
    "convert-icons": "docker run  -v $PWD/design/:/design -v $PWD/dist:/dist --rm --cap-add=SYS_ADMIN  yr-warning-icons"
  },
  "author": "Simen Saegrov",
  "license": "MIT",
  "devDependencies": {
    "@types/puppeteer": "^1.11.1",
    "prettier": "^1.15.3",
    "puppeteer": "^1.11.0",
    "svgo": "^1.1.1"
  },
  "dependencies": {
    "rimraf": "^2.6.3"
  }
}
